{
  "name": "web",
  "version": "1.0.0",
  "description": "management system of three gems",
  "author": "zhong shun",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=80 nuxt",
    "build": "nuxt build",
    "start": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=80 nohup nuxt start < /dev/null >> ./out.log 2>&1 &",
    "stop": "kill -9 $(lsof -t -i:80)",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "fix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore . "
  },
  "dependencies": {
    "@mdi/js": "^4.2.95",
    "@nuxtjs/auth": "^4.8.1",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "nuxt": "^2.9.2",
    "sleep-promise": "^8.0.1",
    "vee-validate": "^2.2.15"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/moment": "^1.3.0",
    "@nuxtjs/vuetify": "^1.8.5",
    "animate.css": "^3.7.2",
    "babel-eslint": "^10.0.3",
    "cross-env": "^5.2.1",
    "eslint": "^6.2.2",
    "eslint-plugin-nuxt": "^0.4.3"
  }
}
